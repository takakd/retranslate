    "next": "^12.1.0",
    "protoc-gen-grpc-web": "^1.3.0",
    "protoc-gen-ts": "^0.8.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-select": "^5.2.2"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
