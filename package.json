    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "npm-check-updates": "^12.4.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
